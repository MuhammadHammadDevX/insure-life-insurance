@model List<Admin>

    <section class="section">
        <div class="section-body">
            <div class="row mt-sm-4">
                <div class="col-12 col-md-12 col-lg-4">
                    <div class="card author-box">
                        <div class="card-body">
                            <div class="author-box-center">
                                <img alt="image" src="@("~/admin_images/"+@Model[0].admin_image)" asp-append-version="true" class="rounded-circle author-box-picture">
                                <br>
                                <br>
                                <div class="clearfix"></div>
                                <div class="author-box-name">
                                    <a>@Model[0].admin_name</a>
                                </div>
                                <br>
                                <div class="author-box-job">MANAGING DIRECTOR & CEO</div>
                            </div>
                            <div class="text-center">
                                <div class="author-box-description">
                                    <p>
                                    Mr. @Model[0].admin_name is the Managing Director and Chief Executive Officer of Insure Life Insurance Limited.
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <div class="card-header">
                            <h4>Personal Details</h4>
                        </div>
                        <div class="card-body">
                            <div class="py-4">
                                <p class="clearfix">
                                    <span class="float-left">
                                        Date of Birth
                                    </span>
                                    <span class="float-right text-muted">
                                        04-07-2002
                                    </span>
                                </p>
                                <p class="clearfix">
                                    <span class="float-left">
                                        Phone
                                    </span>
                                    <span class="float-right text-muted">
                                        @Model[0].admin_phone
                                    </span>
                                </p>
                                <p class="clearfix">
                                    <span class="float-left">
                                        Mail
                                    </span>
                                    <span class="float-right text-muted">
                                        @Model[0].admin_email
                                    </span>
                                </p>
                                <p class="clearfix">
                                    <span class="float-left">
                                        Facebook
                                    </span>
                                    <span class="float-right text-muted">
                                    <a href="https://www.facebook.com/AptechMetro/">@Model[0].admin_name</a>
                                    </span>
                                </p>
                                <p class="clearfix">
                                    <span class="float-left">
                                        Instagram
                                    </span>
                                    <span class="float-right text-muted">
                                    <a href="https://www.instagram.com/aptechmetro/">@Model[0].admin_name</a>
                                    </span>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-12 col-md-12 col-lg-8">
                    <div class="card">
                        <div class="padding-20">
                            <ul class="nav nav-tabs" id="myTab2" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" id="home-tab2" data-toggle="tab" href="#about" role="tab"
                                       aria-selected="true">About</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" id="profile-tab2" data-toggle="tab" href="#settings" role="tab"
                                       aria-selected="false">Setting</a>
                                </li>
                            </ul>
                            <div class="tab-content tab-bordered" id="myTab3Content">
                                <div class="tab-pane fade show active" id="about" role="tabpanel" aria-labelledby="home-tab2">
                                    <div class="row">
                                        <div class="col-md-3 col-6 b-r">
                                            <strong>Full Name</strong>
                                            <br>
                                            <p class="text-muted">@Model[0].admin_name</p>
                                        </div>
                                        <div class="col-md-3 col-6 b-r">
                                            <strong>Phone</strong>
                                            <br>
                                            <p class="text-muted">@Model[0].admin_phone</p>
                                        </div>
                                        <div class="col-md-4 col-7 b-r">
                                            <strong>Email</strong>
                                            <br>
                                            <p class="text-muted">@Model[0].admin_email</p>
                                        </div>
                                        <div class="col-md-2 col-5">
                                            <strong>Location</strong>
                                            <br>
                                            <p class="text-muted">Pakistan</p>
                                        </div>
                                    </div>
                                    <p class="m-t-30">
                                        Completed my graduation in Economics from the well known and
                                        renowned institution
                                        of Pakistan – Bahria Unversity , Karachi in 2000-01, which was
                                        affiliated
                                        to M.S. University. I ranker in University exams from the same
                                        university
                                        from 1996-01.
                                    </p>
                                    <p>
                                        Worked as Professor and Head of the department at Bahria University,
                                        from 2003-2015
                                    </p>
                                    <p>
                                        @Model[0].admin_name is the CEO of Insure Life Insurance and holds a B.A., M.A., and Ph.D. in Economics. They have extensive experience in the insurance industry and a deep understanding of economic strategies. Their leadership has driven the company toward innovation and growth, while their academic background supports informed decision-making and effective business strategies.
                                    </p>
                                    <div class="section-title">Education</div>
                                    <ul>
                                        <li>B.A., Bahria University , Karachi</li>
                                        <li>M.A., FAST University, Karachi</li>
                                        <li>P.H.D., University of Karachi</li>
                                    </ul>
                                    <div class="section-title">Experience</div>
                                    <ul>
                                        <li>
                                            One year experience as Dr. Professor from April-2009 to march-2010
                                            at Bahria University , karachi.
                                        </li>
                                        <li>
                                            four year experience as Dr. Professor at Karachi University &amp; Economic Department
                                            from April - 2010 to May - 2014.
                                        </li>
                                        <li>
                                            Two year experience as Dr. Professor at Karachi University &amp; Economic Department
                                            from April - 2015 to June - 2016.
                                        </li>
                                        <li>
                                            One year experience as Dr. Professor at Karachi University &amp; Economic Department
                                            from April - 2016 to August - 2017.
                                        </li>
                                        <li>
                                           Three year experience as Dr. Professor at Karachi University &amp; Economic Department
                                            from April - 2017 to November - 2020.
                                        </li>
                                        <li>
                                            Four year experience as Dr. Professor at Karachi University &amp; Economic Department
                                            from April - 2020 to December - 2024.
                                        </li>
                                    </ul>
                                </div>
                                <div class="tab-pane fade" id="settings" role="tabpanel" aria-labelledby="profile-tab2">
                                <form method="post" enctype="multipart/form-data">
                                        <div class="card-header">
                                            <h4>Edit Profile</h4>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="form-group col-md-6 col-12">
                                                    <label>Name</label>
                                                    <input type="hidden" class="form-control" name="admin_id" value="@Model[0].admin_id">
                                                    <input type="text" class="form-control" name="admin_name" value="@Model[0].admin_name">
                                                </div>
                                                <div class="form-group col-md-6 col-12">
                                                    <label>Phone</label>
                                                    <input type="text" class="form-control" name="admin_phone" value="@Model[0].admin_phone">
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="form-group col-md-7 col-12">
                                                    <label>Email</label>
                                                    <input type="email" class="form-control" name="admin_email" value="@Model[0].admin_email">
                                                </div>
                                                <div class="form-group col-md-5 col-12">
                                                    <label>Password</label>
                                                    <input type="text" class="form-control" name="admin_password" value="@Model[0].admin_password">
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="form-group col-8">
                                                    <label>Image</label>
                                                    <input type="file" class="form-control" name="admin_image" />
                                                </div>
                                                <div class="form-group col-4">
                                                    <img src="@("~/admin_images/"+@Model[0].admin_image)" asp-append-version="true" width="80px" height="80px" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="card-footer text-right">
                                            <input type="submit" class="btn btn-primary" value="Save Changes" />
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <div class="settingSidebar">
        <a href="javascript:void(0)" class="settingPanelToggle">
            <i class="fa fa-spin fa-cog"></i>
        </a>
        <div class="settingSidebar-body ps-container ps-theme-default">
            <div class=" fade show active">
                <div class="setting-panel-header">
                    Setting Panel
                </div>
                <div class="p-15 border-bottom">
                    <h6 class="font-medium m-b-10">Select Layout</h6>
                    <div class="selectgroup layout-color w-50">
                        <label class="selectgroup-item">
                            <input type="radio" name="value" value="1" class="selectgroup-input-radio select-layout" checked>
                            <span class="selectgroup-button">Light</span>
                        </label>
                        <label class="selectgroup-item">
                            <input type="radio" name="value" value="2" class="selectgroup-input-radio select-layout">
                            <span class="selectgroup-button">Dark</span>
                        </label>
                    </div>
                </div>
                <div class="p-15 border-bottom">
                    <h6 class="font-medium m-b-10">Sidebar Color</h6>
                    <div class="selectgroup selectgroup-pills sidebar-color">
                        <label class="selectgroup-item">
                            <input type="radio" name="icon-input" value="1" class="selectgroup-input select-sidebar">
                            <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                  data-original-title="Light Sidebar"><i class="fas fa-sun"></i></span>
                        </label>
                        <label class="selectgroup-item">
                            <input type="radio" name="icon-input" value="2" class="selectgroup-input select-sidebar" checked>
                            <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                  data-original-title="Dark Sidebar"><i class="fas fa-moon"></i></span>
                        </label>
                    </div>
                </div>
                <div class="p-15 border-bottom">
                    <h6 class="font-medium m-b-10">Color Theme</h6>
                    <div class="theme-setting-options">
                        <ul class="choose-theme list-unstyled mb-0">
                            <li title="white" class="active">
                                <div class="white"></div>
                            </li>
                            <li title="cyan">
                                <div class="cyan"></div>
                            </li>
                            <li title="black">
                                <div class="black"></div>
                            </li>
                            <li title="purple">
                                <div class="purple"></div>
                            </li>
                            <li title="orange">
                                <div class="orange"></div>
                            </li>
                            <li title="green">
                                <div class="green"></div>
                            </li>
                            <li title="red">
                                <div class="red"></div>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="p-15 border-bottom">
                    <div class="theme-setting-options">
                        <label class="m-b-0">
                            <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                   id="mini_sidebar_setting">
                            <span class="custom-switch-indicator"></span>
                            <span class="control-label p-l-10">Mini Sidebar</span>
                        </label>
                    </div>
                </div>
                <div class="p-15 border-bottom">
                    <div class="theme-setting-options">
                        <label class="m-b-0">
                            <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                   id="sticky_header_setting">
                            <span class="custom-switch-indicator"></span>
                            <span class="control-label p-l-10">Sticky Header</span>
                        </label>
                    </div>
                </div>
                <div class="mt-4 mb-4 p-3 align-center rt-sidebar-last-ele">
                    <a href="#" class="btn btn-icon icon-left btn-primary btn-restore-theme">
                        <i class="fas fa-undo"></i> Restore Default
                    </a>
                </div>
            </div>
        </div>
    </div>